<h1 class="page-header">Welcome to Genre Fluid! </h1>
<%= form_tag songs_path, method: "get", html: { class: "form" } do %>
 <div class="form-group col-xs-4">
   <%= text_field_tag(:search_term, "", placeholder: "Enter song name", class: "form-control") %>
 </div>
 <%= submit_tag "Search", class: "btn btn-primary" %>
<% end %>
